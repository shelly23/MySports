<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--
    /*
     *	This content is generated from the API File Info.
     *	(Alt+Shift+Ctrl+I).
     *
     *	@desc
     *	@file 		log_in_page
     *	@date 		Saturday 01st of October 2022 02:28:35 PM
     *	@title 		Page 1
     *	@author
     *	@keywords
     *	@generator 	Export Kit v1.3.figma
     *
     */
    -->

    <ImageView
        android:id="@+id/background_image"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:contentDescription="@string/background_image"
        android:scaleType="fitXY"
        android:src="@drawable/background_image" />


    <RelativeLayout
        android:id="@+id/log_in_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/log_in_fields"
        android:layout_centerHorizontal="true">

        <Button
            android:id="@+id/log_in"
            android:layout_width="260dp"
            android:layout_height="40dp"
            android:layout_marginTop="40dp"
            android:background="@drawable/rectangle_2_shape"
            android:text="@string/log_in_string"
            android:fontFamily="@font/montserrat_regular"
            android:textStyle="bold"
            android:textColor="@color/log_in_color" />

        <TextView
            android:id="@+id/_don_t_have_an_account_"
            android:layout_width="260dp"
            android:layout_height="30dp"
            android:layout_below="@id/log_in"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/montserrat_regular"
            android:text="@string/_don_t_have_an_account__string"
            android:textColor="@color/_bg__log_in_page_ek2_color" />

        <TextView
            android:id="@+id/error_field"
            android:layout_width="260dp"
            android:layout_height="40dp"
            android:layout_below="@id/_don_t_have_an_account_"
            android:layout_marginTop="20dp"
            android:gravity="center_horizontal|center_vertical"
            android:textColor="@color/rectangle_50_color" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/log_in_fields"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/app_info"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp">

        <RelativeLayout
            android:id="@+id/group_ek1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp">

            <EditText
                android:id="@+id/email_address"
                android:layout_width="260dp"
                android:layout_height="40dp"
                android:autofillHints="emailAddress"
                android:fontFamily="@font/montserrat_regular"
                android:hint="@string/email_address_string"
                android:inputType="textEmailAddress"
                android:textColor="@color/email_address_color"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/password_ek1"
                android:layout_width="260dp"
                android:layout_height="40dp"
                android:layout_below="@id/email_address"
                android:layout_marginTop="10dp"
                android:autofillHints="password"
                android:fontFamily="@font/montserrat_regular"
                android:hint="@string/password_ek1_string"
                android:inputType="textPassword"
                android:textColor="@color/email_address_color"
                android:textSize="14sp" />

        </RelativeLayout>


        <TextView
            android:id="@+id/forgot_your_password_"
            android:layout_width="260dp"
            android:layout_height="30dp"
            android:layout_below="@id/group_ek1"
            android:layout_gravity="center_vertical|end"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/montserrat_regular"
            android:gravity="end"
            android:text="@string/forgot_your_password__string"
            android:textColor="@color/_bg__log_in_page_ek2_color" />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/app_info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="180dp">

        <TextView
            android:id="@+id/mysport"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:fontFamily="@font/montserrat_regular"
            android:text="@string/mysport_string"
            android:textColor="@color/rectangle_2_color"
            android:textSize="36sp" />

        <TextView
            android:id="@+id/quote"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/mysport"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/montserrat_regular"
            android:text="@string/quote_string"
            android:textColor="@color/rectangle_2_color"
            android:textSize="14sp" />

    </RelativeLayout>


</RelativeLayout>